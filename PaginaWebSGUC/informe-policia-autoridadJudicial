        function ubicacionTable_toCell(obj,colIdx){
            switch(colIdx){
                case 0:
                    content=document.createTextNode();
                    return content;
                case 1:
                    content=document.createTextNode();
                    return content;
                case 2:
                    content=document.createTextNode();
                    return content;
                };
                return null;
        }



</script>
